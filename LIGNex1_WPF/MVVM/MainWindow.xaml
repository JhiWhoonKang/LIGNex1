<Window x:Class="MVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        DataContext="MVVM.ViewModel.MainWindowViewModel"
        >
    <Grid>
        <TextBox Text="{Binding Model.Num}" HorizontalAlignment="Left" Margin="34,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178" Height="34"/>
        <TextBox Text="{Binding Model.Name}" HorizontalAlignment="Left" Margin="34,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178" Height="40"/>
    </Grid>
</Window>
